{
  "expo": {
    "scheme": "onekey-wallet",
    "name": "HaxWallet",
    "slug": "HaxWallet",
    "owner": "onekeyhq",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#A0C7FD"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.haxwallet.app",
      "associatedDomains": [
        "applinks:app.onekey.so"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#A0C7FD"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "640c3c38-d160-48c3-b4a3-40d4fc8aeb08"
      }
    }
  }
}
